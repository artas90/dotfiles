[
  // -- transform cases -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
  {
    "key": "cmd+k cmd+-",
    "command": "editor.action.transformToKebabcase",
  },
  {
    "key": "cmd+k cmd+p",
    "command": "runCommands",
    "args": {
      "commands": [
        "editor.action.transformToKebabcase",
        "editor.action.transformToPascalcase",
      ]
    }
  },
  {
    "key": "cmd+k cmd+m",
    "command": "runCommands",
    "args": {
      "commands": [
        "editor.action.transformToKebabcase",
        "editor.action.transformToCamelcase",
      ]
    }
  },
  {
    "key": "cmd+k cmd+=",
    "command": "runCommands",
    "args": {
      "commands": [
        "editor.action.transformToSnakecase",
        "editor.action.transformToUppercase",
      ]
    }
  },

  // -- remove default zooming -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -
  {
    "key": "cmd+numpad0",
    "command": "-workbench.action.zoomReset"
  },
  {
    "key": "cmd+=",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "cmd+-",
    "command": "-workbench.action.zoomOut"
  },

  // -- add the same but with shift default zooming -- -- -- -- -- -- -- -- -- -
  {
    "key": "shift+cmd+0",
    "command": "workbench.action.zoomReset"
  },
  {
    "key": "shift+cmd+=",
    "command": "workbench.action.zoomIn"
  },
  {
    "key": "shift+cmd+-",
    "command": "workbench.action.zoomOut"
  },

  // -- new terminal in tab -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -
  {
    "key": "cmd+t",
    "command": "-workbench.action.quickOpen",
  },
  {
    "key": "cmd+t",
    "command": "-workbench.action.showAllSymbols"
  },
  {
    "key": "cmd+t",
    "command": "runCommands",
    "args": {
      "commands": [
        "workbench.action.createTerminalEditor",
        {
          "command": "workbench.action.terminal.sendSequence",
          "args": {
            "text": "  use-cursor-editor; clear \u000D"
          }
        }
      ]
    }
  },

  // -- explorer  -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- --
  {
    "key": "cmd+e",
    "command": "-actions.findWithSelection"
  },
  {
    "key": "cmd+e",
    "command": "-editor.action.toggleScreenReaderAccessibilityMode",
    "when": "accessibilityHelpIsShown"
  },
  {
    "key": "shift+cmd+e",
    "command": "-workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "cmd+e",
    "command": "workbench.view.explorer",
    // "when": "viewContainer.workbench.view.explorer.enabled"
  },

  // -- focusOpenEditorsView -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- - -
  {
    "key": "cmd+o",
    "command": "-workbench.action.files.openFile",
    "when": "false"
  },
  {
    "key": "cmd+o",
    "command": "-workbench.action.files.openFolderViaWorkspace",
    "when": "!openFolderWorkspaceSupport && workbenchState == 'workspace'"
  },
  {
    "key": "cmd+o",
    "command": "-workbench.action.files.openFileFolder",
    "when": "isMacNative && openFolderWorkspaceSupport"
  },
  {
    "key": "cmd+o",
    "command": "workbench.files.action.focusOpenEditorsView",
    "when": "workbench.explorer.openEditorsView.active"
  },

  // -- command-runner -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- - -
  {
    "key": "shift+alt+p",
    "command": "command-runner.run"
  },
  {
    "key": "ctrl+shift+r",
    "command": "-command-runner.run"
  },
  {
    "key": "ctrl+shift+p",
    "command": "command-runner.runInTerminal"
  },
  {
    "key": "ctrl+alt+r",
    "command": "-command-runner.runInTerminal"
  },
]
